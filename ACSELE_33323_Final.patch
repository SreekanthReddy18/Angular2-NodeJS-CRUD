+        try {
+            Session session = AcseleHibernateSessionProvider.getUserDbProvider().getHibernateDbSession();
+            session.save(crmCase);
+            session.flush();
+        } catch (HibernateException e) {
+            log.error(e);
+            throw new TechnicalException(Exceptions.HSErrorSavingObject, e);
+        }
